{
    "name": "kommunikatisten/contao-courses-bundle",
    "version": "0.1.0",
    "type": "contao-bundle",
    "description": "courses bundle for Contao 4",
    "homepage": "https://kommunikatisten.net",
    "license": "MIT",
    "authors": [
        {
            "name": "effeff67",
            "homepage": "https://github.com/kommunikatisten"
        }
    ],
    "require": {
        "php": "^7.0",
        "contao/core-bundle": "^4.8",
        "symfony/framework-bundle": "^4.4"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^4.2"
    },
    "extra": {
        "contao-manager-plugin": "Kommunikatisten\\CoursesBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Kommunikatisten\\CoursesBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kommunikatisten\\CoursesBundle\\Tests\\": "test/"
        }
    },
    "support": {
        "issues": "https://github.com/kommunikatisten/contao-courses-bundle/issues",
        "source": "https://github.com/kommunikatisten/contao-courses-bundle"
    }
}
