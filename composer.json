{
    "name": "kommunikatisten/courses-bundle",
    "type": "contao-bundle",
    "description": "courses bundel for Contao 4",
    "homepage": "https://kommunikatisten.net",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Frank Friedmann",
            "homepage": "https://github.com/kommunikatisten"
        },
        {
            "name": "Contao Community",
            "homepage": "https://contao.org/contributors"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.2",
        "contao/core-bundle": "4.9.*",
        "symfony/framework-bundle": "^3.3"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Kommunikatisten\\CoursesBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Kommunikatisten\\CoursesBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kommunikatisten\\CoursesBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/kommunikatisten/contao_courses_bundle/issues",
        "source": "https://github.com/kommunikatisten/contao_courses_bundle"
    }
}
